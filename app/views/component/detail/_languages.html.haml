- if org.key?(field)
  %section{:class=>css_class_format(field.to_s)}
    - if icon.present?
      %span{"data-icon"=>icon , :class=>"ohana-icon"}
    %h1 #{title}:
    %ul
      - org[field].sort.each do |lang|
        %li
          %span
            - lang_url = /.+\((.+)\)/.match(lang).nil? ? lang : /.+\((.+)\)/.match(lang)[1]
            = link_to lang, organizations_path(@search_params.merge(:language => lang_url))