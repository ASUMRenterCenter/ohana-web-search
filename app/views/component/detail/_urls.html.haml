- if org.key?(:urls)
  %section{:class=>'urls'}
    - if icon.present?
      %span{"data-icon"=>icon , :class=>"ohana-icon"}
    - if (title.present?)
      %h1 #{title}:
    - if org[:urls].kind_of?(Array)
      %ul
        - org[:urls].sort.each do |f|
          %li
            %span
              = format_url(f)